<h3>Sign in</h3>

<%= form_for @user do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "false" %>
  </div>

  <div class="actions">
    <%= f.submit "log in" %>
  </div>
<% end %>
